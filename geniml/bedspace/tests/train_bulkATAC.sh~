path_pipeline='/scratch/bx2ur/code/reninness_score/pipeline'
path_starspace='/scratch/bx2ur/code/reninness_score/tools/Starspace/starspace'
path_meta='/scratch/bx2ur/code/reninness_score/tests/renin_annotation_sheet.csv'
path_universe='/scratch/bx2ur/code/reninness_score/data/universe/universe.bed'
path_output='/scratch/bx2ur/code/reninness_score/outputs/gitk_univ_d3'
assembly='mm10'
path_data='/scratch/bx2ur/code/bedembed/data/all/'
labels="Group"
dim=3
epochs=20
learning_rate=0.001

python $path_pipeline/train_starspace.py -star $path_starspace -i $path_data -g $assembly -meta $path_meta -univ $path_universe \
-l $labels -o $path_output -dim $dim -epochs $epochs -lr $learning_rate

